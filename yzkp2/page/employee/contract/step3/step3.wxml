<view class='pictures'>
  <!-- 寸照 -->
  <view class="post-job">
    <view class='tip'>
      <view></view>
      <text>寸照</text>
    </view>
    <view class='picture cunzhao' data-idx="0" >
      <image src='{{urlData[0]}}' ></image>
      <view class='upload-btn' bindtap='uploadPic'>
        <image src='/image/mine/add.png'></image>
        <text>寸照</text>
      </view>
    </view>
    <text>红底/蓝底</text>
  </view>
  <!-- 户口簿 -->
  <view class="post-job">
    <view class='tip'>
      <view></view>
      <text>户口簿</text>
    </view>
    <view class='picture hukou active'>
      <image src='{{urlData[1]}}'></image>
      <view class='upload-btn' data-idx="1" bindtap='submitBtn'>
        <image src='/image/mine/add.png'></image>
        <text>户口簿-主页</text>
      </view>
    </view>
    <view class='picture hukou' data-idx="2" bindtap='uploadPic'>
      <image src='{{urlData[2]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>户口簿-增减页</text>
      </view>
    </view>
    <view class='picture hukou' data-idx="3" bindtap='uploadPic'>
      <image src='{{urlData[3]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>户口簿-本人页</text>
      </view>
    </view>
  </view>
  <!-- 身份证 -->
  <view class="post-job">
    <view class='tip'>
      <view></view>
      <text>身份证</text>
    </view>
    <view class='picture sfz active' data-idx="4" bindtap='uploadPic'>
      <image src='{{urlData[4]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>身份证正面</text>
      </view>
    </view>
    <view class='picture sfz' data-idx="5" bindtap='uploadPic'>
      <image src='{{urlData[5]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>身份证返面</text>
      </view>
    </view>
  </view>
  <!-- 学历证明 -->
  <view class="post-job">
    <view class='tip'>
      <view></view>
      <text>学历证明</text>
    </view>
    <view class='picture xueli' data-idx="6" bindtap='uploadPic'>
      <image src='{{urlData[6]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>学历证明证书</text>
      </view>
    </view>
  </view>
  <!-- 离职证明 -->
  <view class="post-job" data-idx="7" bindtap='uploadPic'>
    <view class='tip'>
      <view></view>
      <text>离职证明</text>
    </view>
    <view class='picture lizhi'>
      <image src='{{urlData[7]}}'></image>
      <view class='upload-btn'>
        <image src='/image/mine/add.png'></image>
        <text>离职证明材料</text>
      </view>
    </view>
  </view>
  <!-- 其它资格/职称证明 -->
  <view class="post-job">
    <view class='tip'>
      <view></view>
      <text>其它资格/职称证明</text>
    </view>
    <view class='picture other' data-idx="8" bindtap='unploadPic'>
      <image src=''></image>
      <view class='upload-btn'>
        <image src='{{urlData[8]}}'></image>
        <text>其它资格/职称证明</text>
      </view>
    </view>
  </view>
  <view class='post-job-btn'>
    <view class='other-goods'>
      <image src='/image/mine/add.png'></image>
      <text>继续添加</text>
    </view>
  </view>
  <navigator hover-class='none' url='/page/employee/contract/step4/step4'>
    <view class='btn'>下一步</view>
  </navigator>
</view>



