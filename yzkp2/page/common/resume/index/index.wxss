/* page/my/resume/index/index.wxss */
page{
  background: #fff;
}
.header{
  margin-bottom: 0;
  border-bottom: 10rpx solid #e8e8e8
}
.manageintro-nav{
  height: 75rpx;
  display: flex;
  border-bottom: 1rpx solid #E8E8E8;
}
.manageintro-nav-item{
  width: 186rpx;
  border-right: 2rpx solid #E8E8E8;
  font-size: 28rpx;
  color: #868686;
  line-height: 75rpx;
  text-align: center;
}
.manageintro-nav-item.active{
  color: #fff;
  background: #F39800;
}
.manageintro-nav-item:last-of-type{
  border-right: none;
}
.manageintro-cont{
  margin-top: 37rpx;
}
.manageintro-cont-item{
  width: 702rpx;
  background: rgba(56,48,235,0.05);
  border-radius: 10rpx;
  margin: auto;
  padding: 0 26rpx 0 24rpx; 
  box-sizing: border-box;
  margin-bottom: 11rpx;
}
.manageintro-cont-item-tit{
  display: flex;
  height: 70rpx;
  border-bottom: 1rpx solid #E5E5E5;
  justify-content: space-between;
}
.manageintro-cont-item-tit .tit-info,
.manageintro-cont-item-tit .tit-edit{
  display: flex;
}
.manageintro-cont-item-tit .tit-info image{
  width: 31rpx;
  height: 31rpx;
  margin: 21rpx 7rpx 0 0;
}
.manageintro-cont-item-tit .tit-info text{
  font-size: 24rpx;
  color: #353535;
  margin-top: 24rpx;
  display: inline-block;
  height: 24rpx;
  line-height: 24rpx;
}
.manageintro-cont-item-tit .tit-edit image{
  width: 26rpx;
  height: 26rpx;
  margin: 24rpx 4rpx 0 0;
}
.manageintro-cont-item-tit .tit-edit text{
  font-size: 22rpx;
  color: #F39800;
  margin-top: 26rpx;
  display: inline-block;
  height: 22rpx;
  line-height: 22rpx;
}
.manageintro-cont-item-detail{
  padding-bottom: 38rpx;
}
.manageintro-cont-item-detail>text{
  display: block;
}
.inbox{
  height: 26rpx;
  margin-bottom: 16rpx;
}
.inbox:last-of-type{
  margin-bottom: 0;
}
.inbox image{
  width: 26rpx;
  height: 26rpx;
}
.inbox text{
  font-size: 24rpx;
  color: #868686;
  margin-left: 8rpx;
  display: inline-block;
  height: 26rpx;
  line-height: 26rpx;
}
.in-name{
  height: 60rpx;
  margin-top: 9rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #353535;
}
.in-info{
  color: #868686;
  font-size: 24rpx;
  margin-bottom: 19rpx;
  height: 26rpx;
  line-height: 26rpx;
}
.in-info.want{
  margin-top: 30rpx;
}
.la-item{
  padding: 30rpx 0;
  border-top: 1rpx solid #E8E8E8;  
}
.la-item:last-of-type{
  padding-bottom: 0;
}
.la-item:first-of-type{
  border-top: none;
}
.la-item>image{
  display: inline-block;
  width: 22rpx;
  height: 22rpx;
}
.la-item-cont{
  display: inline-block;
  vertical-align: top;
  margin-left: 10rpx;
}
.la-item-cont text{
  display: block;
}
.la-item-date{
  font-size: 24rpx;
  margin-bottom: 18rpx;
  color: #868686;
  height: 24rpx;
  line-height: 24rpx;
}
.la-item-comp{
  font-size: 26rpx;
  margin-bottom: 14rpx;
  color: #353535;
  height: 26rpx;
  line-height: 26rpx;
}
.la-item-intro{
  font-size: 24rpx;
  color: #868686;
  height: 24rpx;
  line-height: 24rpx;
}
/* 简历设置 */
.set-intro{
  padding-top: 24rpx;
}
.set-intro .set-intro-box{
  width:702rpx;
  height:97rpx;
  background:rgba(56,48,235,0.05);
  border-radius:6rpx;
  margin: auto;
  margin-top: 14rpx; 
}
.set-intro .set-intro-box text{
  line-height: 97rpx;
  color: #353535;
  font-size: 30rpx;
  margin-left: 38rpx;
}
.set-intro .set-intro-box switch{
  margin-right: 36rpx;
  line-height: 97rpx;
}
.who-see-item{
  height: 140rpx;
  width: 100%;
  border-bottom: 1rpx solid #e8e8e8;
  box-sizing: border-box;
  padding: 0 24rpx; 
}
.see-me-company{
  display: flex;
  justify-content: space-between;
}
.see-me-company>text{
  display: block;
}
.see-me-company-name{
  font-size: 28rpx;
  color: #353535;
  height: 28rpx;
  line-height: 28rpx;
  margin-top: 36rpx;
}
.see-me-company-date{
  font-size: 24rpx;
  color: #A1A1A1;
  height: 24rpx;
  line-height: 24rpx;
  margin-top: 40rpx;
}
.see-me-company-frt{
  height: 24rpx;
  margin-top: 12rpx;
  line-height: 24rpx;
  color: #A1A1A1;
}
/* 我的申请 */
.my-apply .my-apply-item{
  width:702rpx;
  height:273rpx;
  background:rgba(56,48,235,0.05);
  border-radius:10rpx;
  margin: auto;
  box-sizing: border-box;
  padding-right: 26rpx;
  margin-bottom: 12rpx;
}
.my-apply-item-tit{
  display: flex;
  justify-content: space-between;
  height: 98rpx;
}
.my-apply-item-tit>text{
  display: block;
  font-size: 24rpx;
  color: #6B6B6B;
  margin-top: 38rpx;
  height: 24rpx;
  line-height: 24rpx;
}
.my-apply-item-tit .posi-left{
  height: 37rpx;
  background:rgba(134,134,134,1);
  border-radius:0 19rpx 19rpx 0;
  color: #FEFEFE;
  line-height: 37rpx;
  padding: 0 19rpx 0 23rpx;
  font-size: 20rpx;
  margin-top: 27rpx;
}
.my-apply-item-tit .posi-left.active{
  background: #3830EB;
}
.my-apply-item-name{
  padding-left: 37rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 17rpx;
}
.my-apply-item-name text{
  display: block;
  font-size: 28rpx;
  color: #353535;
}
.my-apply-item-name image{
  width: 45rpx;
  height: 45rpx;
  display: block;
}
.my-apply-item-pay{
  padding-left: 37rpx;
  font-size: 28rpx;
  color: #6B6B6B;
  line-height: 28rpx;
  margin-bottom: 17rpx;
}
.my-apply-item-footer{
  padding-left: 37rpx;
  display: flex;
  justify-content: space-between;
}
.my-apply-item-footer text{
  font-size: 28rpx;
  color: #6B6B6B;
  height: 28rpx;
  line-height: 28rpx;
}
.my-apply-item-footer .my-apply-item-footer-comp{
  margin-top: 10rpx;
}
.my-apply-item-footer .my-apply-item-footer-comment{
  width:157rpx;
  height:46rpx;
  background:rgba(243,152,0,1);
  border-radius:23rpx;
  text-align: center;
  line-height: 46rpx;
  color: #fff;
  font-size: 28rpx;
}