<view class='box'>
  <text>学校名称</text>
  <input placeholder='请输入学校名称' placeholder-style="color:#D7D7D7"></input>
</view>
<view class='box'>
  <text>学历/学位</text>
  <view class='right'>
     <picker range="{{educationArr}}" value="{{eduIndex}}" bindchange="bindLevelSelect">
        <text hidden='{{pickerDefault[0]}}' class='choose'>请选择</text>        
        <text class='choose'>{{workexpress}}</text>
        <image class='next' src='../../../../image/mine/next.png'></image>
     </picker>
  </view>
</view>
<view class='box'>
  <text>入学时间</text>
  <view class='right'>
     <picker mode="selector" range="{{workexpressData}}" value="{{item}}" bindchange="bindDateChangeExpress">
        <text hidden='{{pickerDefault[0]}}' class='choose'>请选择</text>        
        <text class='choose'>{{workexpress}}</text>
        <image class='next' src='../../../../image/mine/next.png'></image>
     </picker>
  </view>
</view>
<view class='box'>
  <text>毕业时间</text>
  <view class='right'>
     <picker mode="selector" range="{{workexpressData}}" value="{{item}}" bindchange="bindDateChangeExpress">
        <text hidden='{{pickerDefault[0]}}' class='choose'>请选择</text>        
        <text class='choose'>{{workexpress}}</text>
        <image class='next' src='../../../../image/mine/next.png'></image>
     </picker>
  </view>
</view>
<view class='box'>
  <text>专业名称</text>
  <input placeholder='请输入专业名称' placeholder-style="color:#D7D7D7"></input>
</view>

<view class='btn'>保存编辑</view>



