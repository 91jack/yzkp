<!-- 求职者 -->
<view class='header'>
  <navigator hover-class='none'>
    <view class='header-cont'>
      <view>
        <image class='company-pic' src=''></image>
      </view>
      <view class='header-intro'>
        <text class='name'>张澜澜</text>
      </view>
    </view>
  </navigator>
</view>
<view >
<block wx:for="{{jobSeekerList}}">
    <navigator hover-class='none' url="{{item.url}}">
      <view class='box sec'>
        <view class='left'>
          <image src='{{item.src}}'></image>
          <text>{{item.text}}</text>
        </view>
        <view class='right'>
          <image src='/image/mine/next.png'></image>
        </view>
      </view>
    </navigator>
    </block>
   
    <navigator hover-class='none' url='/page/my/pages/sendrecode/sendrecode'>
      <view class='box sec'>
        <view class='left'>
          <image src='/image/mine/record.png'></image>
          <text>意见反馈</text>
        </view>
        <view class='right'>
          <image src='/image/mine/next.png'></image>
        </view>
      </view>
    </navigator>
</view>



